{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "pids",
    "eslintcache",
    "tsbuildinfo",
    "pnpm",
    "prettierrc",
    "prettierignore",
    "tsconfig",
    "nextjs",
    "tailwindcss",
    "postcss",
    "prizma",
    "beckend",
    "artem",
    "получить",
    "всех",
    "пользователей",
    "создать",
    "нового",
    "пользователя",
    "посты",
    "постов",
    "пример",
    "моделей",
    "autoincrement",
    "создаем",
    "данные",
    "тестовые",
    "заполнение",
    "esbenp",
    "vscode",
    "eslint",
    "новый",
    "пост",
    "все",
    "пациента",
    "пациентов",
    "управления",
    "система",
    "форма",
    "медицинской",
    "информацией",
    "регистрации",
    "данными"
  ],
  "cSpell.enabledFileTypes": {
    "gitignore": false,
    "prettierignore": false,
    "prisma": true
  },
  "cSpell.ignorePaths": ["README.md"],
  "npm.packageManager": "npm"
}
